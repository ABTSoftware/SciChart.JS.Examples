  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./build",
    "typeRoots": ["./node_modules/@types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
  },
  "include": [
      "server/*.ts"
  ],
  "exclude": [
      "node_modules"
  ]
}
