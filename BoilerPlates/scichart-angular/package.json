{
    "name": "scichart-angular-boilerplate",
    "version": "0.0.0",
    "keywords": [
        "charting",
        "dataviz",
        "angular",
        "scichart"
    ],
    "scripts": {
        "copyWasm": "copy-files-from-to --config copy-files-from-to.json",
        "ng": "ng",
        "start": "npm run copyWasm && ng serve --disable-host-check",
        "build": "npm run copyWasm && ng build",
        "watch": "npm run copyWasm && ng build --watch --configuration development",
        "test": "ng test"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.3.0",
        "@angular/common": "^17.3.0",
        "@angular/compiler": "^17.3.0",
        "@angular/core": "^17.3.0",
        "@angular/forms": "^17.3.0",
        "@angular/platform-browser": "^17.3.0",
        "@angular/platform-browser-dynamic": "^17.3.0",
        "@angular/router": "^17.3.0",
        "copy-files-from-to": "^3.8.0",
        "rxjs": "~7.8.0",
        "scichart": "^4.0.848",
        "scichart-angular": "^0.0.3",
        "tslib": "^2.6.2",
        "zone.js": "~0.14.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.3.0",
        "@angular/cli": "~17.3.0",
        "@angular/compiler-cli": "^17.3.0",
        "@types/jasmine": "~5.1.4",
        "jasmine-core": "~5.1.2",
        "karma": "~6.4.3",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "typescript": "~5.4.2"
    }
}
