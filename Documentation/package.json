{
    "name": "scichart-documentation",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "tsc": "tsc --watch",
        "compile": "tsc && npm run prettier",
        "compileServer": "tsc --project tsconfig.server.json",
        "compileJsSnippets": "tsc --project tsconfig.js.json",
        "build": "npm run compileServer && npm run compile && npm run copySciChart && npm run gen",
        "copySciChart": "copy-files-from-to --config copy-files-from-to.json",
        "start": "node server/server.js",
        "dev": "ts-node server/server.ts",
        "gen": "ts-node server/generateDemoFiles.ts",
        "prettier": "prettier **/*.{js,ts} --write"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "codesandbox": "^2.2.3",
        "express": "^4.17.1",
        "html-entities": "^2.3.3",
        "scichart": "^4.0.0-beta.781"
    },
    "devDependencies": {
        "@types/express": "^4.17.7",
        "@types/request": "^2.48.8",
        "copy-files-from-to": "^2.1.0",
        "prettier": "^3.5.3",
        "ts-node": "^10.9.1",
        "tslint": "^6.1.2",
        "typescript": "^5.8.2"
    }
